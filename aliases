# commands
alias vim='mvim'
alias g='git'
alias gaa='git add -A'
alias gs='git status'
alias gc='git commit'
alias ttytter='perl ~/.bin/ttytter.pl'
alias t='~/.bin/todo.sh -d ~/.todo/todo.cfg'
alias sharediff='git diff | gist -p -t diff | pbcopy'
alias shorten='isgd `pbpaste` | pbcopy'

